Boolean builder then -> Boolean = [
  this => build this: this
  ^ this
]
Boolean builder unless -> Boolean = [
  this not => build this: this
  ^ this not
]

Boolean builder else -> Boolean = [
  this not => build this: this
  ^ this not
]


