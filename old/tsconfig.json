{
  "include": ["src/**/*.ts", "tests/codegen.test.ts"],
  "compilerOptions": {
    "incremental": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "dist",
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}