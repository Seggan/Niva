// декларация типа
type Person name: string age: int
// создания инстанса
person = Person age: 42 name: "sas"
// декларация фунции для типа Person
// с названием renameTo
// и аргументом name
Person renameTo: name = [
  self name: name
]
// вызов функции keyword типа
person renameTo: "new name!"
// вызов двух унарных функций
// echo(name(person))
// доступ к полям это тоже фукнкции
person name echo

